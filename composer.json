{
    "name": "story-song/story-song",
    "require": {
        "slim/slim": "^4.0",
        "slim/psr7": "^1.5",
        "slim/twig-view": "^3.4",
        "doctrine/orm": "^2.16",
        "symfony/cache": "^6.3",
        "doctrine/cache": "^2.2",
        "symfony/framework-bundle": "^6.3",
        "symfony/runtime": "^6.3",
        "symfony/twig-bundle": "^6.3",
        "symfony/security-bundle": "^6.3",
        "symfony/password-hasher": "^6.3",
        "symfony/console": "^6.3",
        "doctrine/doctrine-bundle": "^2.11",
        "doctrine/doctrine-migrations-bundle": "^3.5",
        "doctrine/migrations": "^3.6",
        "easycorp/easyadmin-bundle": "^4.7",
        "symfony/yaml": "^6.3",
        "symfony/dotenv": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "preferred-install": {
            "slim/psr7": "source",
            "*": "dist"
        },
        "allow-plugins": {
            "symfony/runtime": true
        }
    }
}
